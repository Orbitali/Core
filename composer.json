{
    "name": "orbitali/core",
    "description": "Open Source Moduler CMS Project",
    "keywords": [
        "cms"
    ],
    "type": "library",
    "require": {
        "php": "^8.0",
        "butschster/meta-tags": "^1.15",
        "laravel/framework": "8.*",
        "itsgoingd/clockwork": "^5.0",
        "intervention/image": "^2.5",
        "laravel/socialite": "^5.1",
        "laravel/ui": "^3.2.1",
        "silber/bouncer": "v1.0.0-rc.10"
    },
    "autoload": {
        "psr-4": {
            "Orbitali\\": "src"
        },
        "files": [
            "src\\Http\\helpers.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "classmap-authoritative": true,
        "apcu-autoloader": true,
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orbitali\\Providers\\OrbitaliServiceProvider"
            ]
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Umut Akkaya",
            "email": "umutakkaya1996@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
