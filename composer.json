{
    "name": "orbitali/core",
    "description": "Open Source Moduler CMS Project",
    "keywords": [
        "cms"
    ],
    "type": "library",
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.6.*",
        "predis/predis": "^1.1.1",
        "laravel/socialite": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Orbitali\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orbitali\\Providers\\OrbitaliServiceProvider"
            ],
            "aliases": {
                "Orbitali": "Orbitali\\Facades\\Orbitali"
            }
        },
        "include_files": [
            "src/Http/helpers.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Orbitali\\Composer\\Plugin::postAutoloadDump"
        ]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Umut Akkaya",
            "email": "umutakkaya1996@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
