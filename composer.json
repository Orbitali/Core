{
    "name": "orbitali/core",
    "description": "Open Source Moduler CMS Project",
    "keywords": [
        "cms"
    ],
    "type": "library",
    "require": {
        "php": ">=7.1",
        "laravel/framework": "5.7.*",
        "itsgoingd/clockwork": "^3.1",
        "predis/predis": "^1.1.1",
        "laravel/socialite": "^3.1",
        "orbitali/composer-include-files": "dev-master",
        "silber/bouncer": "v1.0.0-rc.2"
    },
    "autoload": {
        "psr-4": {
            "Orbitali\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Orbitali\\Providers\\OrbitaliServiceProvider"
            ],
            "aliases": {
                "Orbitali": "Orbitali\\Facades\\Orbitali"
            }
        },
        "include_files": [
            "src/Http/helpers.php"
        ]
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Umut Akkaya",
            "email": "umutakkaya1996@gmail.com"
        }
    ],
    "minimum-stability": "dev"
}
